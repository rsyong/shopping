header{
	padding-top: 32px;
}
header>div,header>ul{
	line-height: 90px;
}
header div:first-child{
	float: left;
}
header ul{
	float: left;
	margin-left: 100px;
}
header ul li{
	float: left;
	list-style: none;
	padding: 0 30px;
	cursor: pointer;
}
header ul li:hover{
	color: #FFC001;
}
header .active{
	color: #FFC001;
}
footer{
	background: #434343;
	color: #888;
}
footer .container{
	padding: 20px;
	text-align: center;
}
footer .container>a{
	padding: 0 15px;
	font-size: 14px;
	color: #888;
}
footer .container>a:hover{
	color: #fff;
	text-decoration: none;
}
.fo{
	margin-top: 10px;
	color: #888;
}
.loging{
	position: fixed;
	width: 100%;
	background: #262626;
	color: #fff;
	height: 32px;
	line-height: 32px;
	z-index: 100;
	top: 0;
}
.loging ul li{
	float: left;
	list-style: none;
	padding: 0 12px;
	font-size: 14px;
	cursor: pointer;
}
.loging ul{
	float: right;
	padding: 0;
	margin: 0;
}
.loging ul li:hover{
	color: #FFC001;
}
